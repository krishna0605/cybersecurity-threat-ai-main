{
  "version": 2,
  "builds": [
    {
      "src": "src/app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "maxLambdaSize": "15mb",
        "installCommand": "pip install -e ."
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/src/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/app.py"
    }
  ]
} 